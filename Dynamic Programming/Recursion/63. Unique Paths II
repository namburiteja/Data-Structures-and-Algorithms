class Solution {
    public int noOfPaths(int[][] nums,int i,int j){
        if(i<0 || j<0) return 0;
        if(nums[i][j]==1) return 0;
        if(i==0 && j==0) return 1;
        int up = noOfPaths(nums,i-1,j);
        int left = noOfPaths(nums,i,j-1);
        return up+left;
    }
    public int uniquePathsWithObstacles(int[][] obstacleGrid) {
        int m = obstacleGrid.length;
        int n = obstacleGrid[0].length;
        return noOfPaths(obstacleGrid,m-1,n-1);
    }
}
TIME COMPLEXITY - O(2**m*n)
SPACE COMPLEXITY - O(m*n)
