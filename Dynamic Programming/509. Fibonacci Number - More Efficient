class Solution {
    public int fib(int n) {
        if(n==1) return 1;
        int ans = 0;
        int a = 0;
        int b = 1;
        for(int i=2;i<n+1;i++){
            ans = a+b;
            a = b;
            b = ans;
        }
        return ans;
    }
}
