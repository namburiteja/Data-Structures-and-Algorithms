class Solution {
    public int houseRob(int[] nums,int i,int[] dp){
        if(i==0) return nums[i];
        if(i==1) return Math.max(nums[i],nums[0]);
        if(dp[i]!=-1) return dp[i];
        int pick = nums[i] + houseRob(nums,i-2,dp);
        int notpick = houseRob(nums,i-1,dp);
        return dp[i] = Math.max(pick,notpick);
    } 
    public int rob(int[] nums) {
        int n = nums.length;
        if(n==1) return nums[0];
        int[] oneArr = new int[n-1];
        int[] twoArr = new int[n-1];
        for(int i=0;i<n-1;i++){
            oneArr[i] = nums[i];
            twoArr[i] = nums[i+1];
        }
        int[] memo = new int[n-1];
        int[] memo2 = new int[n-1];
        Arrays.fill(memo,-1);
        Arrays.fill(memo2,-1);
        int one = houseRob(oneArr,n-2,memo);
        int two = houseRob(twoArr,n-2,memo2);
        return Math.max(one,two);
    }
}
TIME COMPLEXITY - O(n)
SPACE COMPLEXITY - O(n)
