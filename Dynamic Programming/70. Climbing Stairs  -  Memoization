class Solution {
    public int climbing(int n,int[] dp){
        if(n<=2) return n;
        if(dp[n]!=0) return dp[n];
        int a = climbing(n-1,dp);
        int b = climbing(n-2,dp);
        dp[n] = a+b;
        return dp[n];
    }
    public int climbStairs(int n) {
        int[] dp = new int[n+1];
        return climbing(n,dp);
    }
}
