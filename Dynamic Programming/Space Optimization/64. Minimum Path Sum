class Solution {
    public int minPathSum(int[][] grid) {
        int m = grid.length;
        int n = grid[0].length;
        int[] dp = new int[n]; 
        dp[0] = grid[0][0];
        for(int i=0;i<m;i++){
            int[] temp = new int[n];
            for(int j=0;j<n;j++){
                if(i==0 && j==0) temp[j] = grid[i][j];
                else{
                    int up = (int) Math.pow(10,9);
                    int down = (int) Math.pow(10,9);
                    if(i>0) up = grid[i][j] + dp[j];
                    if(j>0) down = grid[i][j] + temp[j-1];
                    temp[j] = Math.min(up,down);
                }
            }
            dp = temp;
        }
        return dp[n-1];
    }
}
TIME COMPLEXITY - O(m*n)
SPACE COMPLEXITY - O(n)
