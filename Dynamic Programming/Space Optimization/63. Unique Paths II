class Solution {
    public int uniquePathsWithObstacles(int[][] obstacleGrid) {
        int m = obstacleGrid.length;
        int n = obstacleGrid[0].length;
        int[] dp = new int[n];
        dp[0] = 1;
        if(obstacleGrid[0][0]==1) return 0;
        for(int i=0;i<m;i++){
            int[] temp = new int[n];
            for(int j=0;j<n;j++){
                if(obstacleGrid[i][j]==1){
                    temp[j] = 0;
                    continue;
                }
                if(i==0 && j==0) temp[j]=1;
                else{
                    if(i>0) temp[j] += dp[j];
                    if(j>0) temp[j] += temp[j-1];
                }
            }
            dp = temp;
        }
        return dp[n-1];
    }
}
