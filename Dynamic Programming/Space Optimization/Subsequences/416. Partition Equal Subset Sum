class Solution {
    public boolean canPartition(int[] arr) {
        int s=0;
        int n = arr.length;
        for(int i=0;i<n;i++) s+=arr[i];
        if(s%2!=0) return false;
        int s1 = s/2+1;
        int[] memo = new int[s1];
        for(int i=0;i<n;i++) memo[0] = 1;
        if(s1>=arr[0]) memo[arr[0]] = 1;
        for(int i=1;i<n;i++){
            int[] temp = new int[s1];
            temp[0] = 0;
            for(int target=1;target<s1;target++){
                boolean nottake  = (memo[target] == 1);
                boolean take = false;
                if(target>=arr[i]) take  = (memo[target-arr[i]]==1);
                temp[target] = (take || nottake) ? 1 : 0;
            }
            memo = temp;
        }
        return memo[s1-1] == 1;
    }
}
TIME COMPLEXITY - O(n*target)
SPACE COMPLEXITY - O(target)
