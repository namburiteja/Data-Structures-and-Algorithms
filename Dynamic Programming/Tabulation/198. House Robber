class Solution {
    public int rob(int[] nums) {
        int n = nums.length;
        int[] memo = new int[n];
        memo[0] = nums[0];
        if(n>1) memo[1] = Math.max(nums[0],nums[1]);
        for(int i=2;i<n;i++){
            int take = nums[i] + memo[i-2];
            int nottake = memo[i-1];
            memo[i] = Math.max(take,nottake);
        }
        return memo[n-1];
    }
}
TIME COMPLEXITY - O(n)
SPACE COMPLEXITY - O(n)
