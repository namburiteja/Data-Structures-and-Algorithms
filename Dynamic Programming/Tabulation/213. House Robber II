class Solution {
    public int rob(int[] nums) {
        int n = nums.length;
        if(n==1) return nums[0];
        int[] memo = new int[n-1];
        int[] memo2 = new int[n-1];
        memo[0] = nums[0];
        if(n>2) memo[1] = Math.max(nums[0],nums[1]);
        for(int i=2;i<n-1;i++){
            int pick = nums[i] + memo[i-2];
            int notpick = memo[i-1];
            memo[i] = Math.max(pick,notpick);
        }
        memo2[0] = nums[1];
        if(n>2) memo2[1] = Math.max(nums[1],nums[2]);
        for(int i=2;i<n-1;i++){
            int pick = nums[i+1] + memo2[i-2];
            int notpick = memo2[i-1];
            memo2[i] = Math.max(pick,notpick);
        }
        return Math.max(memo[n-2],memo2[n-2]);
    }
}
TIME COMPLEXITY - O(n)
SPACE COMPLEXITY - O(n)
