class Solution {
    public boolean canPartition(int[] arr) {
        int s=0;
        int n = arr.length;
        for(int i=0;i<n;i++) s+=arr[i];
        if(s%2!=0) return false;
        int s1 = s/2+1;
        int[][] memo = new int[n][s1];
        for(int i=0;i<n;i++) memo[i][0] = 1;
        if(s1>=arr[0]) memo[0][arr[0]] = 1;
        for(int i=1;i<n;i++){
            for(int target=1;target<s1;target++){
                boolean nottake  = (memo[i-1][target] == 1);
                boolean take = false;
                if(target>=arr[i]) take  = (memo[i-1][target-arr[i]]==1);
                memo[i][target] = (take || nottake) ? 1 : 0;
            }
        }
        return memo[n-1][s1-1] == 1;
    }
}
TIME COMPLEXITY - O(n*target)
SPACE COMPLEXITY - O(n*target)
